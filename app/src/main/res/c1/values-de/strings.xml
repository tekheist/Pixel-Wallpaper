  <string name="appearance_light_text">Immer hellen Text nutzen</string>
  <string name="appearance_light_text_description">Statt dunklem Text auf hellen Hintergründen</string>
  <string name="appearance_dark_text">Dunklen Text nutzen</string>
  <string name="appearance_dark_text_description">Nur auf ausreichend hellen Hintergründen</string>
  <string name="appearance_colors_category">Farben</string>
  <string name="appearance_colors">Extrahierte Farben</string>
  <string name="appearance_colors_description">Wird zur Berechnung des Textkontrasts verwendet</string>
  <string name="appearance_colors_description_dynamic">Werden von dem Design-Generator des Systems genutzt</string>
  <string name="appearance_colors_description_one_ui">Wie oben beschrieben, ignoriert One UI die von Live-Hintergründen bereitgestellten Farben</string>
  <string name="appearance_colors_primary">Primäre Farbe auswählen</string>
  <string name="appearance_colors_secondary">Sekundäre Farbe auswählen</string>
  <string name="appearance_colors_tertiary">Tertiäre Farbe auswählen</string>
